{
  "name": "Bandcamp Fanpage Albumcolors",
  "description": "Addes Albumcolors to Bandcamp.com Fanpages",
  "manifest_version": 2,
  "version": "0.0.1",
  "permissions": ["http://*/*"],
  "minimum_chrome_version": "22",
  "content_scripts": [{
    "matches": ["http://bandcamp.com/*"],
    "js": ["jquery.js","app.js"]
  }],
  "background": {
    "scripts": ["albumcolors.js","background.js"],
    "persistent": false
  }
}
